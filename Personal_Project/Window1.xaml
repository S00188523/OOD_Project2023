<Window x:Class="Personal_Project.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Personal_Project"
        mc:Ignorable="d"
        Title="Team Info" Height="450" Width="800"
        Loaded="Window_Loaded">
    <Grid >
        <TextBlock x:Name="tblkTeamName" HorizontalAlignment="Left" Margin="310,112,0,0" TextWrapping="Wrap" Text="{Binding Name}" VerticalAlignment="Top"/>
    </Grid>
</Window>
